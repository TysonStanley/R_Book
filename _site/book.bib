journal = {Journal of physical activity {\&} health},
keywords = {Adolescent,Cardiovascular health,Measurement,Metabolic health,Physical activity assessment,Survey research},
number = {5},
pages = {708--16},
pmid = {24909801},
title = {{Self-Reported Versus Accelerometer-Measured Physical Activity and Biomarkers Among NHANES Youth.}},
url = {http://www.scopus.com/inward/record.url?eid=2-s2.0-84939164087{\&}partnerID=tZOtx3y1},
volume = {12},
