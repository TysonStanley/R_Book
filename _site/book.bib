journal = {Trials},
keywords = {Analysis of Variance,Computer Simulation,Data Interpretation,Humans,Linear Models,Logistic Models,Models,Proportional Hazards Models,Randomized Controlled Trials as Topic,Randomized Controlled Trials as Topic: statistics,Reproducibility of Results,Research Design,Research Design: statistics {\&} numerical data,Statistical},
number = {1},
pages = {139},
pmid = {24755011},
title = {{The risks and rewards of covariate adjustment in randomized trials: an assessment of 12 outcomes from 8 studies}},
