journal = {Journal of Experimental Social Psychology},
keywords = {Cognitive psychology,Experimentation,Expertise,Replication,Social psychology},
pages = {181--184},
publisher = {Elsevier B.V.},
title = {{Does expertise matter in replication? An examination of the reproducibility project: Psychology}},
