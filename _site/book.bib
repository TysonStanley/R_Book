@article{Scaramuzza2010,
abstract = {AIM: The aim of the student is to assess whether adolescents with type 1 diabetes mellitus (T1DM) in Italy differ from their healthy peers in regard to risky behaviour. METHODS: Data were collected from 215 patients, aged 14 +/- 2 years with a mean disease duration of 7 +/- 5 years. The control group was comprised of 464 healthy adolescents recruited among high school students. Each patient completed an anonymous confidential questionnaire to determine the prevalence of sexual behaviour, alcohol and tobacco consumption, illicit drug use, and, among patients with diabetes and frequency of mismanagement related to diabetes care. RESULTS: Compared with controls, subjects with diabetes showed a similar rate of sexual intercourse among males and lower rates among females (34.8{\%} vs 35.5{\%}, p NS and 29.4{\%} vs 41.4{\%}, p {\textless} 0.05, respectively). Males in the diabetes group reported a higher rate of tobacco use, whereas females showed similar or higher rates of use for every illicit drug studied. Among patients with diabetes, those who are engaged in risky behaviour showed a higher rate of treatment mismanagement (76{\%} vs 34{\%}, p {\textless} 0.01). CONCLUSION: Adolescents with T1DM are as likely as their healthy peers to engage in risky behaviour, indicating the potential benefit of anticipatory guidance concerning glycaemic control and increased risk of acute and chronic complications.},
author = {Scaramuzza, Ae and {De Palma}, a and Mameli, C and Spiri, D and Santoro, L and Zuccotti, G V},
doi = {10.1111/j.1651-2227.2010.01813.x},
file = {:Users/tysonbarrett/Library/Application Support/Mendeley Desktop/Downloaded/Scaramuzza et al. - 2010 - Adolescents with type 1 diabetes and risky behaviour.pdf:pdf},
isbn = {1651-2227 (Electronic)$\backslash$r0803-5253 (Linking)},
issn = {1651-2227},
journal = {Acta paediatrica},
keywords = {adolescence,alcohol,illicit drugs,sexuality,smoking,type 1 diabetes},
