keywords = {Cohort Studies,Diagnosis-Related Groups,Female,Follow-Up Studies,Food, Fortified,Gastrointestinal Diseases,Gastrointestinal Diseases: complications,Gastrointestinal Diseases: economics,Home Care Services,Home Care Services: economics,Hospital Costs,Hospitals, Teaching,Humans,Insurance, Health, Reimbursement,Length of Stay,Liver Diseases,Liver Diseases: complications,Liver Diseases: economics,Male,Nutrition Assessment,Nutrition Disorders,Nutrition Disorders: economics,Nutrition Disorders: epidemiology,Nutrition Disorders: mortality,Nutritional Status,Ohio,Patient Discharge,Patient Readmission,Prospective Studies,Risk Factors,Survival Analysis},
