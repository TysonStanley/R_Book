@article{Reid2013,
abstract = {Variance estimation in the linear model when p {\textgreater} n is a difficult problem. Standard least squares estimation techniques do not apply. Several variance estimators have been proposed in the literature, all with accompanying asymptotic results proving consistency and asymptotic normality under a variety of assumptions. It is found, however, that most of these estimators suffer large biases in finite samples when true underlying signals become less sparse with larger per element signal strength. One estimator seems to be largely neglected in the literature: a residual sum of squares based estimator using Lasso coefficients with regularisation parameter selected adaptively (via cross-validation). In this paper, we review several variance estimators and perform a reasonably extensive simulation study in an attempt to compare their finite sample performance. It would seem from the results that variance estimators with adaptively chosen regularisation parameters perform admirably over a broad range of sparsity and signal strength settings. Finally, some intial theoretical analyses pertaining to these types of estimators are proposed and developed.},
archivePrefix = {arXiv},
arxivId = {arXiv:1311.5274v1},
author = {Reid, Stephen and Tibshirani, Robert and Friedman, Jerome H.},
eprint = {arXiv:1311.5274v1},
file = {:Users/tysonbarrett/Library/Application Support/Mendeley Desktop/Downloaded/Reid, Tibshirani, Friedman - 2013 - A study of error variance estimation in lasso regression.pdf:pdf},
journal = {arXiv preprint},
keywords = {cross-validation,error variance estimation,lasso},
pages = {1--25},
title = {{A study of error variance estimation in lasso regression}},
url = {http://arxiv.org/abs/1311.5274},
year = {2013}
}
@article{Society2007,
abstract = {We propose a new method for estimation in linear models. The `lasso' minimizes the residual sum of squares subject to the sum of the absolute value of the coefficients being less than a constant. Because of the nature of this constraint it tends to produce some coefficients that are exactly 0 and hence gives interpretable models. Our simulation studies suggest that the lasso enjoys some of the favourable properties of both subset selection and ridge regression. It produces interpretable models like subset selection and exhibits the stability of ridge regression. There is also an interesting relationship with recent work in adaptive function estimation by Donoho and Johnstone. The lasso idea is quite general and can be applied in a variety of statistical models: extensions to generalized regression models and tree-based models are briefly described.},
