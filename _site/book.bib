@article{Williams2015,
abstract = {This study proposes a cross-sectional model of positive family and peer relationships on school misbehavior and health and well-being in adolescence. Students (n = 20,749, 50.6 {\%} girls, 43.9 {\%} black, 67 {\%} middle school, 56 {\%} free/reduced price lunches) completed the school success profile in the classroom. A structural equation model was estimated using Mplus. After controlling for non-independence of observations due to clustering (n = 318 schools) and individual-level variables (age, socio-economic status, gender), family togetherness ($\beta$ = .52, p {\textless} .001), parent behavioral expectations ($\beta$ = .07, p {\textless} .05), and friend support ($\beta$ = .12, p {\textless} .001) were associated with greater health and well-being; parent behavioral expectations ($\beta$ = −.25, p {\textless} .001) and friend support ($\beta$ = −.11, p {\textless} .001) were associated with less school misbehavior. Family relationships were mediated through adolescents' susceptibility to peer pressure (health and well-being, $\beta$ = −.34, p {\textless} .001; school misbehavior, $\beta$ = .16, p {\textless} .001). That families and friends play critical roles in different adolescent outcomes and work through an individual vulnerability suggests a number of practice implications that are discussed. (PsycINFO Database Record (c) 2015 APA, all rights reserved). (journal abstract)},
